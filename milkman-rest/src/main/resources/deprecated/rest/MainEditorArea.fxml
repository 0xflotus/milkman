<HBox xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="milkman.ui.plugin.rest.RestRequestEditController"
	HBox.hgrow="ALWAYS">
	<JFXComboBox fx:id="httpMethod" value="GET">
		<items>
			<FXCollections fx:factory="observableArrayList">
				<String fx:value="GET" />
				<String fx:value="POST" />
				<String fx:value="PUT" />
				<String fx:value="DELETE" />
				<String fx:value="PATCH" />
				<String fx:value="HEAD" />
				<String fx:value="OPTIONS" />
			</FXCollections>
		</items>
	</JFXComboBox>
	<JFXTextField fx:id="requestUrl" HBox.hgrow="ALWAYS" />
</HBox>